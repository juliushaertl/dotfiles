URxvt.saveLines: 10000
URxvt.letterSpace: 0
URxvt.iso14755_52: false
URxvt.iso14755: false
URxvt.scrollBar: false
URxvt*urgentOnBell: true
*.URxvt.urgentOnBell: true
URxvt.secondaryScroll: true
URxvt.font: xft:Source Code Pro-8

urxvt*perl-lib:         /usr/lib/urxvt/perl/
URxvt.perl-ext-common: default,font-size,keyboard-select,clipboard,url-select,new-window
URxvt.url-launcher: /usr/bin/google-chrome-stable
URxvt.matcher.button: 1
URxvt.keysym.C-u: perl:matcher:last
URxvt.keysym.M-u: perl:matcher:list

URxvt.keysym.Control-Shift-N: perl:new-window

URxvt.keysym.C-0x2b:     perl:font-size:increase
URxvt.keysym.C-0x2d:   perl:font-size:decrease
URxvt.keysym.C-A-0x2b:   perl:font-size:incglobal
URxvt.keysym.C-A-0x2d: perl:font-size:decglobal

URxvt.url-select.launcher:  google-chrome-stable
URxvt.url-select.underline: true
URxvt.keysym.M-u:           perl:url-select:select_next
URxvt.keysym.M-Escape:      perl:keyboard-select:activate
URxvt.keysym.M-s:           perl:keyboard-select:search

URxvt.keysym.Control-Up:   \033[1;5A
URxvt.keysym.Control-Down:    \033[1;5B
URxvt.keysym.Control-Left:    \033[1;5D
URxvt.keysym.Control-Right:    \033[1;5C


urxvt.print-pipe: "cat > /dev/null"

URxvt.color0: #000000
URxvt.color1: #C75646
URxvt.color2: #8EB33B
URxvt.color3: #D0B03C
URxvt.color4: #72B3CC
URxvt.color5: #C8A0D1
URxvt.color6: #218693
URxvt.color7: #B0B0B0
URxvt.color8: #5D5D5D
URxvt.color9: #E09690
URxvt.color10: #CDEE69
URxvt.color11: #FFE377
URxvt.color12: #9CD9F0
URxvt.color13: #FBB1F9
URxvt.color14: #77DFD8
URxvt.color15: #F7F7F7

URxvt.foreground: #FFFFFF
URxvt.background: rgba:2400/2400/2400/dcdc

Xcursor.theme: Adwaita
